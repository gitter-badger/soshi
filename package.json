{
  "name": "soshi",
  "version": "0.0.23",
  "description": "an xperimental alternatives to webcomponents",
  "main": "build/soshi.js",
  "scripts": {
    "test": "xo",
    "build": "webpack --config webpack.build.config.js",
    "start": "webpack-dev-server --content-base build --hot --inline"
  },
  "xo": {
    "space": true,
    "rules": {
      "import": {
        "noUnresolved": false
      },
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ]
    },
    "globals": [
      "document",
      "window"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stagfoo/soshi.git"
  },
  "author": "Alexander Ravenking",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stagfoo/soshi/issues"
  },
  "homepage": "https://github.com/stagfoo/soshi#readme",
  "devDependencies": {
    "@types/node": "^8.0.54",
    "awesome-typescript-loader": "^3.4.0",
    "source-map-loader": "^0.2.3",
    "standard-version": "^4.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.4",
    "xo": "^0.18.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "tslint": "^5.8.0",
    "sheetify": "^6.2.0",
    "awesome-typescript-loader": "^3.4.1"
  },
  "dependencies": {
    "mdn-polyfills": "^5.5.0",
    "yo-yo": "^1.4.1"
  }
}
